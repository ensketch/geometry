#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBENSKETCH_GEOMETRY_VERSION       $libensketch_geometry.version.project_number$ULL
#define LIBENSKETCH_GEOMETRY_VERSION_STR   "$libensketch_geometry.version.project$"
#define LIBENSKETCH_GEOMETRY_VERSION_ID    "$libensketch_geometry.version.project_id$"
#define LIBENSKETCH_GEOMETRY_VERSION_FULL  "$libensketch_geometry.version$"

#define LIBENSKETCH_GEOMETRY_VERSION_MAJOR $libensketch_geometry.version.major$
#define LIBENSKETCH_GEOMETRY_VERSION_MINOR $libensketch_geometry.version.minor$
#define LIBENSKETCH_GEOMETRY_VERSION_PATCH $libensketch_geometry.version.patch$

#define LIBENSKETCH_GEOMETRY_PRE_RELEASE   $libensketch_geometry.version.pre_release$

#define LIBENSKETCH_GEOMETRY_SNAPSHOT_SN   $libensketch_geometry.version.snapshot_sn$ULL
#define LIBENSKETCH_GEOMETRY_SNAPSHOT_ID   "$libensketch_geometry.version.snapshot_id$"
